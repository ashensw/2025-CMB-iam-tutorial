{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "resolveJsonModule": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@pet-management-webapp/data-access-common-api-util": [
        "data-access/data-access-common-api-util/index.ts"
      ],
      "@pet-management-webapp/data-access-common-models-util": [
        "data-access/data-access-common-models-util/index.ts"
      ],
      "@pet-management-webapp/data-access-controller": [
        "data-access/data-access-controller/index.ts"
      ],
      "@pet-management-webapp/ui-assets": [
        "ui/ui-assets"
      ],
      "@pet-management-webapp/ui-components": [
        "ui/ui-components/index.ts"
      ],
      "@pet-management-webapp/util-application-config-util": [
        "util/util-application-config-util/index.ts"
      ],
      "@pet-management-webapp/util-authorization-config-util": [
        "util/util-authorization-config-util/index.ts"
      ],
      "@pet-management-webapp/shared/data-access/data-access-common-api-util": [
        "shared/data-access/data-access-common-api-util/index.ts"
      ],
      "@pet-management-webapp/shared/data-access/data-access-common-models-util": [
        "shared/data-access/data-access-common-models-util/index.ts"
      ],
      "@pet-management-webapp/shared/ui/ui-assets": [
        "shared/ui/ui-assets"
      ],
      "@pet-management-webapp/shared/ui/ui-basic-components": [
        "shared/ui/ui-basic-components/index.ts"
      ],
      "@pet-management-webapp/shared/ui/ui-components": [
        "shared/ui/ui-components/index.ts"
      ],
      "@pet-management-webapp/shared/util/util-application-config-util": [
        "shared/util/util-application-config-util/index.ts"
      ],
      "@pet-management-webapp/shared/util/util-authorization-config-util": [
        "shared/util/util-authorization-config-util/index.ts"
      ],
      "@pet-management-webapp/shared/util/util-common": [
        "shared/util/util-common/index.ts"
      ],
      "@pet-management-webapp/shared/util/util-front-end-util": [
        "shared/util/util-front-end-util/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
